/* SLIDE DOCUMENTCLUOD */
	/*	iframe */
	iframe#documentcloud { width: 100%; height: 750px; overflow: none }

	#app, #app div#slide, #app ul#list li {height: 750px; width: 100%}
	#app { overflow: hidden; position: relative;  margin: 20px 0;}
	#app a.btn, #app div#slide { position: absolute; top: 0}
	#app a.btn { background: rgba(52, 158, 205,.7); display: block; cursor: pointer; z-index: 1; display: inline-block; padding: 5px 10px; top: 50px;}
	#app a.btn {display: none}
  #app a.btn:hover { background: rgba(52, 158, 205,1);}
	#app a.prev {/* background-image: ;*/ left: 0}
	#app a.next { /*background-image: ;*/ right: 0}
	#app div#slide { overflow: hidden; margin: 0 6px; left:0px; position: relative;}
	#app ul#list { overflow: hidden;}
	#app ul#list li { overflow: hidden; position: absolute; top: 0; left: 0; text-align: center; width: 99%; overflow-y: auto}
/* /SLIDE DOCUMENTCLUOD */


